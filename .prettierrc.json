{
	"printWidth": 80,
	"tabWidth": 4,
	"useTabs": true,
	"semi": true,
	"trailingComma": "es5",
	"endOfLine": "lf",
	"singleQuote": false,
	"quoteProps": "as-needed",
	"jsxSingleQuote": false,
	"embeddedLanguageFormatting": "auto",
	"bracketSpacing": true,
	"bracketSameLine": false,
	"arrowParens": "avoid",
	"proseWrap": "preserve",
	"htmlWhitespaceSensitivity": "strict",
	"singleAttributePerLine": true,
	"overrides": [
		{
			"files": "*.json",
			"options": {
				"printWidth": 120
			}
		}
	]
}
